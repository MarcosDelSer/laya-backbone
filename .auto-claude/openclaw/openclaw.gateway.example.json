{
  "gateway": {
    "mode": "local",
    "bind": "tailnet",
    "auth": {
      "mode": "token",
      "token": "REPLACE_WITH_STRONG_GATEWAY_TOKEN"
    }
  },
  "tools": {
    "exec": {
      "host": "node",
      "security": "allowlist",
      "node": "linux-e2e-01"
    }
  },
  "browser": {
    "enabled": true,
    "defaultProfile": "browserless",
    "remoteCdpTimeoutMs": 2000,
    "remoteCdpHandshakeTimeoutMs": 4000,
    "profiles": {
      "browserless": {
        "cdpUrl": "https://production-sfo.browserless.io?token=REPLACE_BROWSERLESS_TOKEN",
        "color": "#00AA00"
      }
    }
  },
  "observability": {
    "provider": "langfuse",
    "endpoint": "https://cloud.langfuse.com",
    "publicKeyEnv": "LANGFUSE_PUBLIC_KEY",
    "secretKeyEnv": "LANGFUSE_SECRET_KEY"
  },
  "notes": {
    "required_env": [
      "ANTHROPIC_API_KEY",
      "OPENCLAW_GATEWAY_TOKEN",
      "LANGFUSE_PUBLIC_KEY",
      "LANGFUSE_SECRET_KEY",
      "BROWSERLESS_TOKEN"
    ],
    "optional_env": [
      "BROWSERBASE_API_KEY",
      "BROWSERSTACK_USERNAME",
      "BROWSERSTACK_ACCESS_KEY",
      "SAUCE_USERNAME",
      "SAUCE_ACCESS_KEY",
      "AWS_ACCESS_KEY_ID",
      "AWS_SECRET_ACCESS_KEY"
    ]
  }
}
